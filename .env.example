# Frontend Environment Variables Template
# Copy this file to .env and fill in your values
# All variables MUST start with REACT_APP_ to be accessible

# API Configuration
REACT_APP_API_URL=http://localhost:5555

# App Information
REACT_APP_NAME=Space for Rent
REACT_APP_VERSION=1.0.0

# Map Configuration (Leaflet)
REACT_APP_DEFAULT_LAT=40.7128
REACT_APP_DEFAULT_LNG=-74.0060
REACT_APP_DEFAULT_ZOOM=12

# Feature Flags
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_REVIEWS=true
